{
  "name": "graasp-app-sketch",
  "version": "0.1.4",
  "private": true,
  "dependencies": {
    "@material-ui/core": "3.5.1",
    "@material-ui/icons": "3.0.1",
    "detect-browser": "3.0.1",
    "downloadjs": "1.4.7",
    "prop-types": "15.6.2",
    "react": "16.6.3",
    "react-color": "2.14.1",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1",
    "react-sketch": "0.4.4"
  },
  "homepage": ".",
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn build:react && yarn build:bundle",
    "build:react": "react-scripts build",
    "build:bundle": "webpack --config webpack.config.js --mode=production",
    "build:local": "env-cmd ./.env.local yarn build",
    "build:dev": "env-cmd ./.env.dev yarn build",
    "build:prod": "env-cmd ./.env.prod yarn build",
    "build:version": "REACT_APP_VERSION=$CI_BRANCH env-cmd --no-override ./.env.prod yarn build",
    "lint": "eslint . && remark .",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "cross-env CI=true env-cmd ./.env.test react-scripts test --env=jsdom --coverage",
    "report": "cat ./coverage/lcov.info | env-cmd ./.env.test codacy-coverage",
    "eject": "react-scripts eject",
    "release": "standard-version"
  },
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "codacy-coverage": "3.3.0",
    "cross-env": "5.2.0",
    "env-cmd": "8.0.2",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "glob": "7.1.3",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.2.0",
    "interpolate-html-plugin": "3.0.0",
    "path": "0.12.7",
    "remark-cli": "6.0.1",
    "remark-lint": "6.0.4",
    "remark-preset-lint-recommended": "3.0.2",
    "script-ext-html-webpack-plugin": "2.1.3",
    "standard-version": "4.4.0",
    "uglifyjs-webpack-plugin": "1.2.7",
    "webpack-cli": "3.1.2"
  },
  "release": {
    "analyzeCommits": "conventional"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
